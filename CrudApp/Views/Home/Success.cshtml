
@{
    Layout = "~/Views/Shared/_StormLayout1.cshtml";
}
<div class="success">
    <div class="clear-heading lean-left-2">
        <h1>Success</h1>
    </div>
</div>

@section Scripts{
    <script>
    $(document).ready(function () {
        function setHeight() {
            windowHeight = $(window).innerHeight();
            $('.success').css('min-height', windowHeight);
        };
        setHeight();

        $(window).resize(function () {
            setHeight();
        });
    });
</script>
    
    }


